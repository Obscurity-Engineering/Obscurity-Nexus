{
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts, ${capture}.stories.tsx",
        "tsconfig.json": "tsconfig.*.json, tailwind.config.js, .eslintrc.json, next.config.js",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, next-env.d.ts, .gitignore",
        "README.md": "LICENSE",
    },
    "cSpell.words": [
        "autodocs"
    ]
}